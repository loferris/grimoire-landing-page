{
  "name": "grimoire-landing-page",
  "version": "1.0.0",
  "description": "A landing page for grimoire.app during initial development",
  "main": "index.js",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "deploy": "gatsby build --prefix-paths && gh-pages -b master -d public",
    "deploy:github": "npm run build && node ./scripts/deploy-github",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loferris/grimoire-landing-page.git"
  },
  "keywords": [
    "grimoire"
  ],
  "author": "Lo Ferris",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/loferris/grimoire-landing-page/issues"
  },
  "homepage": "https://github.com/loferris/grimoire-landing-page#readme",
  "dependencies": {
    "@emotion/core": "^10.0.21",
    "@emotion/styled": "^10.0.17",
    "@okta/okta-auth-js": "^2.9.0",
    "babel-polyfill": "^6.26.0",
    "core-js": "^3.3.2",
    "draft-js": "^0.11.1",
    "draft-js-image-plugin": "^2.0.7",
    "draft-js-plugins-editor": "^3.0.0",
    "gatsby": "^4.25.7",
    "gatsby-plugin-emotion": "^4.1.12",
    "gatsby-plugin-typography": "^2.3.14",
    "immutable": "^4.0.0-rc.12",
    "jimp": "^0.8.5",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-imgix": "^8.6.4",
    "react-typography": "^0.16.19",
    "typescript": "^3.6.4",
    "typography": "^0.16.19",
    "typography-theme-moraga": "^0.16.19"
  },
  "devDependencies": {
    "gh-pages": "^2.1.1",
    "prettier": "*"
  }
}
